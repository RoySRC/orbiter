	cd $(SRC); make clean; git pull; make 


conf_10_3:
	$(REGULAR_LS_PATH)/regular_ls.out -v 2 -m 10 -n 10 -k 3 -r 3 \
		-starter_size 10 -input_prefix "./" -solution_prefix "./" \
		-base_fname "conf_10_3" \
		-starter


