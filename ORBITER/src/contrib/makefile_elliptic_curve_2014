MY_PATH=../../DEV.14/
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2


CRYPTO_PATH=$(SRC2)/CRYPTO
SEMIFIELD_PATH=$(SRC)/SEMIFIELD
TREEDRAW_PATH=$(SRC2)/TREEDRAW
DRAW_PATH=$(SRC2)/DRAW




4_1_1:
	$(CRYPTO_PATH)/crypto.out -v 3 -q 4 -b 1 -c 1 
 
8_1_1:
	$(CRYPTO_PATH)/crypto.out -v 3 -q 8 -b 1 -c 1 
 
16_1_1:
	$(CRYPTO_PATH)/crypto.out -v 3 -q 16 -b 1 -c 1 
 

7_6_2:
	$(CRYPTO_PATH)/crypto.out -v 3 -q 7 -b 6 -c 2 
 

draw:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file codes_24_12_2_8_poset_lvl_24.layered_graph \
		-draw Golay24 \
		-xin 10000 -yin 10000 \
		-xout 1000000 -yout 1000000 \
		-y_stretch 1.2 \
		-nodes_empty \
		-embedded
	pdflatex Golay24.tex
	open Golay24.pdf


