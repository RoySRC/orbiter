#3 : 3 orbits
#4 : 4 orbits
#5 : 49 orbits
#6 : 9461 orbits
# Nov 26, 2013 on MacBookPro: 3 min 33 sec

#7 : 2493211 orbits
# Nov 27, 2013 on MacBookPro: 5 hrs 9 min 59 sec


A: lift_0_25 lift_2_25 lift_4_25 lift_6_25 lift_8_25 lift_10_25 \
	lift_12_25 lift_14_25 lift_16_25 lift_18_25 lift_20_25 \
	lift_22_25 lift_24_25


B: lift_1_25 lift_3_25 lift_5_25 lift_7_25 lift_9_25 lift_11_25 \
	lift_13_25 lift_15_25 lift_17_25 lift_19_25 \
	lift_21_25 lift_23_25



lift_0_25:
	- mkdir SYSTEMS
	$(STEFAAN_PATH)/stefaan_main.out -v 2 \
		-n $(N) -k $(K) -q $(Q) -W \
		-starter_directory_name STARTER/ \
		-starter_prefix stefaan_$(Q) \
		-lift $(STARTER_DEPTH) SYSTEMS/ \
		-save \
		-solve \
		-split 0 25 >log_0_25


