#MY_PATH=../../DEV.15
MY_PATH=/scratch/betten/COMPILE/2015_05_26
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

STEFAAN_PATH=$(SRC2)/STEFAAN

# Mathon's system of 21 lines in PG(5,3) 


N=6
K=2
Q=3
STARTER_DEPTH=6
POLY=


clean:
	-rm -rf SOLUTIONS STARTER SYSTEMS log_*

starter:
	- mkdir SYSTEMS
	$(STEFAAN_PATH)/stefaan_main.out -v 5 \
		-n $(N) -k $(K) -q $(Q) -W \
		-starter_directory_name STARTER/ \
		-starter_prefix stefaan_$(Q) \
		-starter $(STARTER_DEPTH)


# May 24, 2015 on MacBookPro: STARTER_DEPTH=6, 2 min 47 , 9461 orbits

#0 : 1 orbits
#1 : 1 orbits
#2 : 1 orbits
#3 : 3 orbits
#4 : 5 orbits
#5 : 58 orbits
#6 : 10065 orbits
# Nov 20, 2013 on MacBookPro: 4 min 12 sec


#7 : 2775913 orbits
# Nov 20, 2013 on domino: 5 hrs 29 min 




