MY_PATH=../..
SRC=$(MY_PATH)/SRC
SRC2=$(MY_PATH)/SRC2

LINEAR_SET_PATH=$(SRC2)/LINEAR_SET


2_8:
	$(LINEAR_SET_PATH)/linear_set.out -n 2 -v 2 -q 2 -Q 8

4_64:
	$(LINEAR_SET_PATH)/linear_set.out -n 2 -v 2 -q 4 -Q 64 -dim 3 -W

# starting with a group of order 262080,
# we get a group of order 21 * 262080 = 5503680

8:
	$(LINEAR_SET_PATH)/linear_set.out -n 4 -v 5 -q 2 -Q 8 -dim 6


# 25 sec
# 10 orbits, stabilizer orders: (3402, 63^2, 18, 6^4, 2^2)


27:
	$(LINEAR_SET_PATH)/linear_set.out -n 4 -v 6 -q 3 -Q 27 -dim 6


