MY_PATH=../../../..
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

DUAL_HYPEROVAL_PATH=$(SRC2)/DUAL_HYPEROVAL
GRASSMANN_PATH=$(SRC2)/GRASSMANN



K=4
N=7
Q=2
DEPTH=6
POLY=



starter:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 5 -W -n $(N) -k $(K) -q $(Q)\
		-starter -depth $(DEPTH) -W

# MacBook May 27, 2013:  40 sec 

#Found 10032 orbits at depth 6
#0 : 1 orbits
#1 : 1 orbits
#2 : 1 orbits
#3 : 1 orbits
#4 : 4 orbits
#5 : 114 orbits
#6 : 10032 orbits
#total: 10154
## of calls to image_of/mult/invert/retrieve/store: 21400/914617/755636/759455/50229
#(48, 24, 20, 18, 12, 8^7, 6^{12}, 4^{32}, 3^{36}, 2^{294}, 1^{9646}) average is 1 + 688 / 10032


lift:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 3 -W -n $(N) -k $(K) -q $(Q)\
		-depth $(DEPTH) \
		-lift $(DEPTH) ./ \
		-solve -lex \
		-split 0 2000 \
		-memory_debug


# MacBook May 24, 2013:  13 min 56 sec (with -lex)
#total_solutions = 4852
#nb_deleted_solutions=3202
# writes file ./DH_16_depth_6_solutions.txt



build_db:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-build_db 

# MacBook May 24, 2013:  20 sec


read_solutions:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-read_solution_files DH_16_depth_6_solutions.txt -1


# MacBook May 24, 2013:  16 sec

compute_orbits:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-compute_orbits 

# MacBook May 24, 2013:  1 sec
#isomorph::orbits_of_stabilizer finished, we found 4702 orbits : 4^9, 3^2, 2^119, 1^4572

isomorph:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-isomorph_testing  -print_interval 250

# MacBook May 24, 2013:  1 min 30 sec, finds 37 isomorphism types


report:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-report 

magma:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-export_magma 


D1:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-D1 

identify:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-identify \
			QUOTIENTS_16/quotient_q2_nb0.txt \
