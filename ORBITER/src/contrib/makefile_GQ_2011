	$(MY_PATH)/TDO/tdo_refine.out -v 2 -nopacking  GQ_3_5_32_d.tdo
	$(MY_PATH)/TDO/tdo_print.out -v 2 -tex GQ_3_5_32_dr.tdo
	$(MY_PATH)/TDO/tdo_refine.out -v 2 -nopacking  GQ_3_5_32_dr.tdo
	$(MY_PATH)/TDO/tdo_print.out -v 2 -tex GQ_3_5_32_drr.tdo
	$(MY_PATH)/TDO/tdo_refine.out -v 2 -nopacking  GQ_3_5_32_drr.tdo
