refine:
	$(MY_PATH)/WIDOR/widor.out -v 2  GQ_3_5_32_dd.widor
	$(MY_PATH)/TDO/tdo_print.out -v 2 -tex GQ_3_5_32_dd.tdo
	$(MY_PATH)/TDO/tdo_refine.out -v 2 -nopacking  GQ_3_5_32_dd.tdo
	$(MY_PATH)/TDO/tdo_print.out -v 2 -tex GQ_3_5_32_ddr.tdo
	$(MY_PATH)/TDO/tdo_refine.out -v 2 -nopacking  GQ_3_5_32_ddr.tdo
	$(MY_PATH)/TDO/tdo_print.out -v 2 -tex GQ_3_5_32_ddrr.tdo



