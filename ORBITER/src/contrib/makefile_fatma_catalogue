# writes surface_catalogue_q4_0_points.txt, which needs hand-editing

canon4_0:
	$(PROJECTIVE_SPACE_PATH)/canonical_form.out -v 10 -n 3 -q 4 \
		-input -file_of_points surface_catalogue_q4_0_points.txt -end \
		-classify_nauty \
		-prefix surface_catalogue_q4_0_points \
		-save surface_catalogue_q4_0_points \
		-latex 
	pdflatex surface_catalogue_q4_0_points_classification.tex
	open surface_catalogue_q4_0_points_classification.pdf



