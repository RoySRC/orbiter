24_12_8:
	$(CODES_PATH)/codes.out -v 2 -n 24 -k 12 -q 2 -d 8 -lex 


24_12_8_draw:
	$(CODES_PATH)/codes.out -v 6 -n 24 -k 12 -q 2 -d 8 -w -lex \
		-draw_poset \
		-export_schreier_trees \
		-tools_path $(GRAPH_THEORY_PATH) \
		-report
	latex codes_linear_n24_k12_q2_d8.tex
	latex codes_linear_n24_k12_q2_d8.tex
	dvips codes_linear_n24_k12_q2_d8.dvi -o
	open codes_linear_n24_k12_q2_d8.ps


