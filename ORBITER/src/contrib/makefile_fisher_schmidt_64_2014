MY_PATH=../../../DEV.14
#MY_PATH=/scratch/betten/COMPILE/2014_12_02
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

PHILIP_PATH=$(SRC)/FISHER_SCHMIDT
TREEDRAW_PATH=$(SRC)/TOOLS
ALGEBRA_PATH=$(SRC2)/ALGEBRA
PROJECTIVE_SPACE_PATH=$(SRC2)/PROJECTIVE_SPACE


FIELD=-Q 4096 -poly_Q "4331" -q 64 -poly_q "91"

SMALLFIELD=-q 64 -poly "91"

