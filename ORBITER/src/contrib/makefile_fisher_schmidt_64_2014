#####################
#  PG(2,64)
#####################


F64:
	$(ALGEBRA_PATH)/cheat_sheet_GF.out -q 64 -poly "91"
	pdflatex GF_64.tex
	open GF_64.pdf
	



matrix:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 3 $(FIELD) -aut 1 

# Dec 23, 2014: the normalizer has order 24 (59 min 3 sec)
# new MacBookPro Dec 14, 2014: 0 sec (with pair test)
# new MacBookPro Dec 12, 2014: 3 sec

c:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 $(FIELD) -aut 1 \
		-load  \
		-clique \
		-orbits 2 cliques 

#		-tree search_64 


#\
#		-tree search_64 


# Dec 9, 2014: 0 sec
# writes file FisherSchmidt_solution_64_1.txt

# Aug 19, 2014 on MacBook 4 sec, 36 solutions, 20 hyperovals
#Written file FisherSchmidt_hyperoval_64_1.txt of size 6266

p:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 $(FIELD) -aut 1 -load \
		-process FisherSchmidt_solution_64_1.txt



conic_type:
	$(PROJECTIVE_SPACE_PATH)/process_PG.out -v 2 $(SMALLFIELD) \
		-conic_type  -n 2 -file FisherSchmidt_hyperoval_64_1.txt


#The conic intersection type is:
#8^10998 9^1168 10^172 11^10 13^1 
#\begin{align*}
#\begin{array}{r|*{1}{r}}
#\rightarrow  &      1_{  1}\\
#\hline
#    13_{  0} &            1\\
#    53_{  2} &            0\\
#\end{array}
#\end{align*}


#The conic intersection type is:
#8^10830 9^1390 10^246 12^10 13^5 
#\begin{align*}
#\begin{array}{r|*{1}{r}}
#\rightarrow  &      5_{  1}\\
#\hline
#     1_{  0} &            5\\
#    60_{  2} &            1\\
#     5_{  3} &            0\\
#\end{array}
#\end{align*}

#\begin{align*}
#\begin{array}{r|*{1}{r}}
#\rightarrow  &      1_{  1}\\
#\hline
#    13_{  0} &            1\\
#    53_{  2} &            0\\
#\end{array}
#\end{align*}

conic_type_random:
	$(PROJECTIVE_SPACE_PATH)/process_PG.out -v 2 $(SMALLFIELD) \
		-conic_type -randomized 100000 -n 2 -file FisherSchmidt_hyperoval_64_1.txt



64draw:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 $(FIELD) -aut 1 \
		-load -draw \
		-clique \
		-process 
	pdflatex FisherSchmidt_matrix_64_1_bitmatrix.tex
	open FisherSchmidt_matrix_64_1_bitmatrix.pdf


#nb_points=183
#nb_colors=7

64tree:
	$(TREEDRAW_PATH)/treedraw.out \
		-x 1000000 -y 1000000 \
		-on_circle -rad 20000 -circle \
		-file search_64.tree   \
		-embedded 
	pdflatex search_64.tex
	open search_64.pdf

#		-no_circletext


64s:
	- $(PHILIP_PATH)/fisher_schmidt_main.out -v 2 $(FIELD) -aut 1 \
		-load  \
		-clique \
		-use_symmetry_group 2 


# writes FisherSchmidt_solution_64_1_case_0_0.txt

#		-tree search_64 


64r:
	- $(PHILIP_PATH)/fisher_schmidt_main.out -v 2 $(FIELD) -aut 1 \
		-load  \
		-clique \
		-use_symmetry_group 2 \
		-read_instead

# writes FisherSchmidt_solution_64_1_case_0_0.arcs

64sp:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 $(FIELD) -aut 1 -load \
		-process FisherSchmidt_solution_64_1_case_0_0.arcs





