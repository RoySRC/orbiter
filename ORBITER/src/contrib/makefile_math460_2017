MY_PATH=../../DEV.17
#MY_PATH=/scratch2/betten/COMPILE/2016_01_18
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

BCH_PATH=$(SRC2)/BCH_AND_TENSOR
THE_PATH=$(SRC2)/MATH460
TOOLS_PATH=$(SRC)/APPS/TOOLS
COMBINATORICS_PATH=$(SRC)/APPS/COMBINATORICS

code8:
	$(THE_PATH)/binary_code.out -n 8 -a 103 -c 47 -seed 90 -r 5
	
code5:
	$(THE_PATH)/binary_code.out -n 5 -a 13 -c 47 -seed 90 -r 5
	
darts_0:
	$(THE_PATH)/darts.out -a 5 -c 11 -m 997 -seed 7 -N 0 -bullseye 5 -label dart_0 -subdivide 10
	pdflatex dart_0.tex
	open dart_0.pdf


darts_1:
	$(THE_PATH)/darts.out -a 31 -c 3 -m 7919 -seed 3 -N 1000 -bullseye 5 -label dart_1
	pdflatex dart_1.tex
	open dart_1.pdf

darts_2:
	$(THE_PATH)/darts.out -a 3 -c 3 -m 7919 -seed 3 -N 500 -bullseye 5 -label dart_2
	pdflatex dart_2.tex
	open dart_2.pdf

darts_3:
	$(THE_PATH)/darts.out -a 5 -c 11 -m 997 -seed 7 -N 10 -bullseye 5 -label dart_3
	pdflatex dart_3.tex
	open dart_3.pdf


darts_4:
	$(THE_PATH)/darts.out -a 1103515245 -c 12345 -m 2147483647 -seed 3 -N 500 -bullseye 5 -label dart_4
	pdflatex dart_4.tex
	open dart_4.pdf

#a=1103515245	c=12345


Hamming:
	$(THE_PATH)/hamming_bound.out 


BCH15:
	$(BCH_PATH)/BCH.out -n 15 -q 2 -BCH 3 1
	$(BCH_PATH)/BCH.out -n 15 -q 2 -BCH 5 1
	$(BCH_PATH)/BCH.out -n 15 -q 2 -BCH 7 1
	$(BCH_PATH)/BCH.out -n 15 -q 2 -BCH 9 1


BCH11:
	$(BCH_PATH)/BCH.out -n 11 -q 2 -BCH 3 1
	$(BCH_PATH)/BCH.out -n 11 -q 2 -BCH 5 1


BCH13:
	$(BCH_PATH)/BCH.out -n 13 -q 2 -BCH 3 1
	$(BCH_PATH)/BCH.out -n 13 -q 2 -BCH 5 1


BCH7:
	$(BCH_PATH)/BCH.out -n 7 -q 2 -BCH 3 1
	$(BCH_PATH)/BCH.out -n 7 -q 2 -BCH 5 1


BCH21:
	$(BCH_PATH)/BCH.out -n 21 -q 2 -BCH 3 1
	$(BCH_PATH)/BCH.out -n 21 -q 2 -BCH 5 1
	$(BCH_PATH)/BCH.out -n 21 -q 2 -BCH 7 1


subsets4:
	$(COMBINATORICS_PATH)/rank_subsets_lex.out -n 4
	$(TOOLS_PATH)/treedraw.out -file subsets_of_4.tree -embedded -circletext -circle -rad 23000 
	pdflatex subsets_of_4.tex
	open subsets_of_4.pdf

bernoulli4:
	$(TOOLS_PATH)/treedraw.out -file bernoulli_4.tree \
		-y_out 500000 \
		-embedded -circle -rad 11000 
	pdflatex bernoulli_4.tex
	open bernoulli_4.pdf

#-circletext 

bernoulli5:
	$(TOOLS_PATH)/treedraw.out -file bernoulli_5.tree \
		-x_out 1000000 \
		-y_out 500000 \
		-embedded  -circle -rad 5000 
	pdflatex bernoulli_5.tex
	open bernoulli_5.pdf

#-circletext


tree4:
	$(COMBINATORICS_PATH)/rank_anything.out -n 4 -binary_trees
	$(TOOLS_PATH)/treedraw.out -file binary_tree_of_depth_4.tree -embedded \
		-x_out 1000000 \
		-y_out 500000 \
		-circle -rad 5000 
	pdflatex binary_tree_of_depth_4.tex
	open binary_tree_of_depth_4.pdf

#-circletext 

tree7:
	$(COMBINATORICS_PATH)/rank_anything.out -n 7 -binary_trees
	$(TOOLS_PATH)/treedraw.out -file binary_tree_of_depth_7.tree -embedded \
		-x_out 1200000 \
		-y_out 500000 \
		-circle -rad 2000 \
		-line_width 0.4 \
		-scale 0.4
	pdflatex binary_tree_of_depth_7.tex
	open binary_tree_of_depth_7.pdf

