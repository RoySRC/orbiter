#-print_level 5


poset:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file subspaces_10_2_5_poset_lvl_5.layered_graph \
		-draw subspaces_10_2_5_poset_lvl_5 \
		-rad 1000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-nodes_empty 
	pdflatex subspaces_10_2_5_poset_lvl_5.tex
	open subspaces_10_2_5_poset_lvl_5.pdf



tree:
	$(TREEDRAW_PATH)/treedraw.out  -file subspaces_10_2_5_10.tree  -circle  -rad 20000
	pdflatex subspaces_10_2_5_10.tex
	open subspaces_10_2_5_10.pdf

poset_32:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file subspaces_10_2_5_poset_lvl_10.layered_graph \
		-draw subspaces_10_2_5_poset_lvl_10 \
		-rad 90 \
		-embedded
		pdflatex subspaces_10_2_5_poset_lvl_10.tex
		open subspaces_10_2_5_poset_lvl_10.pdf


