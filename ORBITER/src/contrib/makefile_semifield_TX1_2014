compute:
	$(SEMIFIELD_PATH)/semifield_main.out -v 3 \
		-order $(ORDER) \
		-recoordinatize \
		-prefix semi_$(ORDER) \
		-W \
		-break_symmetry 3 


deep:
	$(SEMIFIELD_PATH)/semifield_main.out -v 2 \
		-order $(ORDER) \
		-recoordinatize \
		-prefix semi_$(ORDER) \
		-W \
		-deep_search 3 0 1 

aut:
	$(SEMIFIELD_PATH)/semifield_main.out -v 2 \
		-order $(ORDER) \
		-compute_automorphism_group \
			3 34630287489 17315143801 1108650321 3074286151 5317831282 9571344270 -1


