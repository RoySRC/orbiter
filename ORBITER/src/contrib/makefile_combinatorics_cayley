MY_PATH=~/DEV.19/GITHUB/orbiter
#MY_PATH=/scratch3/betten/COMPILE/2018_12_25
SRC=$(MY_PATH)/ORBITER/src
SRC2=$(MY_PATH)/ORBITER2/src

ALGEBRA_PATH=$(SRC)/apps/algebra
GROUPS_PATH=$(SRC)/apps/groups
PROJECTIVE_SPACE_PATH=$(SRC)/apps/projective_space
SUBSPACE_ORBITS_PATH=$(SRC)/apps/subspace_orbits
GRAPH_THEORY_PATH=$(SRC)/apps/graph_theory




cayley_s3s:
	$(GRAPH_THEORY_PATH)/cayley_sym_n.out -v 1 -n 3 -star
	$(GRAPH_THEORY_PATH)/draw_colored_graph.out -v 1 -file Cayley_Sym_3_special.colored_graph -export Cayley_Sym_3_special.txt
	$(GRAPH_THEORY_PATH)/draw_colored_graph.out -v 1 -file Cayley_Sym_3_special.colored_graph -aut -on_circle -embedded -scale 0.25 -line_width 0.5
	$(GRAPH_THEORY_PATH)/draw_colored_graph.out -v 1 -file Cayley_Sym_3_special.colored_graph -aut -bitmatrix -embedded -scale 0.25 -line_width 0.5
	pdflatex Cayley_Sym_3_special_on_circle.tex
	open Cayley_Sym_3_special_on_circle.pdf
	pdflatex Cayley_Sym_3_special_bitmatrix.tex
	open Cayley_Sym_3_special_bitmatrix.pdf

