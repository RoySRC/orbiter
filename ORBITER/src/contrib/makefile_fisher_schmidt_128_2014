#####################
#  PG(2,128)
#####################



matrix:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 16384 -poly_Q "16553" -q 128 -poly_q "131" -aut 1


# new MacBookPro Dec 12, 2014: 4 min 43


#nb_points=998
#nb_colors=11
# 497503 entries in the adjacency matrix
# Aug 18, 2014 on MacBook 13:52, 20 solutions, 10 hyperovals
# Aug 18, 2014 on domino 10:17, 20 solutions, 10 hyperovals

# after optimizing:
# Aug 18, 2014 on MacBook 5:32, 20 solutions, 10 hyperovals
# Aug 18, 2014 on MacBook 5:30, 20 solutions, 10 hyperovals


128:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 16384 -poly_Q "16553" -q 128 -poly_q "131" -aut 1 -load -draw


# new MacBookPro Dec 12, 2014: 1 sec
# Nov 4, 2014 on MacBook in 7 sec, 20 solutions, 10 hyperovals
# Nov 5, 2014 on MacBook in 29 sec, 10 solutions, 10 hyperovals (with triple test)


128r:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 16384 -poly_Q "16553" -q 128 -poly_q "131" -aut 1 -load \
		-restrictions 0 0 10 3 0 1000 -1 \
		-clique


matrix128_2:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 16384 -poly_Q "16553" -q 128 -poly_q "131" -aut 2




128_4:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 16384 -poly_Q "16553" -q 128 -poly_q "131" -aut 2 -load 





