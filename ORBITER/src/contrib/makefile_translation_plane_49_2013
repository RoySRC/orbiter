MY_PATH=../../..
#MY_PATH=/scratch/betten/COMPILE/2013_02_21
SRC=$(MY_PATH)/SRC
SRC2=$(MY_PATH)/SRC2

TRANSLATION_PLANE_PATH=$(SRC2)/TRANSLATION_PLANE
GRASSMANN_PATH=$(SRC2)/GRASSMANN
DIOPHANT_PATH=$(SRC)/DIOPHANT
COMBINATORICS_PATH=$(SRC2)/COMBINATORICS


Q=7
ORDER=49
DEPTH=7
POLY=
SEARCH_DEPTH=3
PACKING_SEARCH_DEPTH=4









starter:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 5 -starter -W -order $(ORDER) -depth $(DEPTH)


