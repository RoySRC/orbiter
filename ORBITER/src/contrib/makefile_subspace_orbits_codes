ALGEBRA_PATH=$(SRC2)/ALGEBRA
ANGELES_PATH=$(SRC2)/ANGELES


clean:
	- rm codes_*
	- rm Golay*
	- rm Hamming*
	- rm log a x.log


#########

PG_3_2:
	$(PROJECTIVE_SPACE_PATH)/cheat_sheet_PG.out -v  2 -n 3 -q 2
	pdflatex PG_3_2.tex
	open PG_3_2.pdf



######### length 3


3_2t:
	$(SUBSPACE_ORBITS_PATH)/subspace_orbits_main.out -v 3 \
		-depth 3 -group -PGL 3 2 -identity_group -end \
		-draw_poset -embedded \
		-list \
		-table_of_nodes

3_2td:
	$(GRAPH_THEORY_PATH)/layered_graph_main.out -v 4 \
		-file Identity_3_2_poset_lvl_3.layered_graph \
		-draw \
		-rad 23000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-scale .30 \
		-line_width 1.0 \
		-y_stretch 0.8 \
		-select_layer 0 \
		-select_layer 1 \
		-select_layer 2 \
		-select_layer 3 
	pdflatex Identity_3_2_poset_lvl_3_draw.tex
	open Identity_3_2_poset_lvl_3_draw.pdf



