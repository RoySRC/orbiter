		-draw test \
		-rad 2000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1500000 \
		-yout 1500000 \
		-embedded \
		-scale .40 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex PGL_3_2_poset_detailed_lvl_4_draw.tex
	open PGL_3_2_poset_detailed_lvl_4_draw.pdf



