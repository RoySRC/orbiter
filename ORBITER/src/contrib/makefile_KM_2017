PSL_2_11:
	$(KRAMER_MESNER_PATH)/km.out -v 2 -linear -PSL 2 11 -end -t 2 -k 3 -KM




PGL_2_11:
	$(KRAMER_MESNER_PATH)/km.out -v 2 -linear -PGL 2 11 -end -t 7 -k 12 -KM -draw_poset -embedded
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file PGL_2_11_poset_lvl_12.layered_graph \
		-draw test \
		-rad 6000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1500000 \
		-yout 1500000 \
		-embedded \
		-scale .30 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex PGL_2_11_poset_lvl_12_draw.tex
	open PGL_2_11_poset_lvl_12_draw.pdf


cheat_1_11:
	$(PROJECTIVE_SPACE_PATH)/cheat_sheet_PG.out -n 1 -q 11
	pdflatex PG_1_11.tex
	open PG_1_11.pdf

