MY_PATH=../../DEV.17
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2


TOOLS_PATH=$(SRC)/APPS/TOOLS
COMBINATORICS_PATH=$(SRC)/APPS/COMBINATORICS
PROJECTIVE_SPACE_PATH=$(SRC)/APPS/PROJECTIVE_SPACE
SUBSPACE_ORBITS_PATH=$(SRC2)/SUBSPACE_ORBITS
KRAMER_MESNER_PATH=$(SRC2)/KRAMER_MESNER


PGL_4_13:
	$(KRAMER_MESNER_PATH)/km.out -v 2 -linear -PGL 4 13 -end -t 2 -k 3 -KM




4_2_wedge:
	$(KRAMER_MESNER_PATH)/km.out -v 2 -linear -PGL 4 2 -wedge -end -t 2 -k 3 -KM 

