PGL_4_2:
	$(KRAMER_MESNER_PATH)/km.out -v 2 -linear -PGL 4 2 -on_k_subspaces 2 -end -t 2 -k 3 -KM -draw_poset -embedded -surface
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file PGL_4_2_Grassmann_4_2_2_poset_lvl_3.layered_graph \
		-draw test \
		-rad 8000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-scale .30 \
		-line_width 1.0 \
		-y_stretch 0.8 
