search_lex:
	$(TRANSLATION_PLANE_PATH)/tp_extend.out -v 6 -order $(ORDER)  -lex $(DEPTH) 0 1


# Dec 8, 2011: DEPTH=8 gives 1608 solutions in 8 min 58 on MacBook
# Dec 8, 2011: DEPTH=7 gives 3025 solutions in 56 sec on MacBook
# Dec 8, 2011: DEPTH=6 gives 9444 solutions in 5 sec on MacBook
# Dec 8, 2011: DEPTH=5 gives 19541 solutions in 1 sec on MacBook
# (the number of solutions can be seen from the extend...txt file)

search_lex_0:
	$(TRANSLATION_PLANE_PATH)/tp_extend.out -v 6 -order $(ORDER)  -lex $(DEPTH) 0 10000


build_db:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 2 -build_db $(DEPTH) \
		-order $(ORDER) 

read:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 2 \
		-read_solution_files $(DEPTH) extend_spread_$(ORDER)_$(DEPTH)_0_1.txt -1 \
		-order $(ORDER) 

orbits:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 2 \
		-compute_orbits $(DEPTH) \
		-order $(ORDER) 

# DEPTH=5: 1176 orbits

iso:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 4 \
		-isomorph_testing $(DEPTH) \
		-order $(ORDER) 

#		-play_back event_save.txt \

print:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 4 \
		-print $(DEPTH) \
		-order $(ORDER) 

