	pdflatex spread_$(ORDER)_19.tex
	pdflatex spread_$(ORDER)_20.tex
	pdflatex spread_$(ORDER)_21.tex


e:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 5 -extend_simple 210 54 673 517 361 -1 -order $(ORDER) 

