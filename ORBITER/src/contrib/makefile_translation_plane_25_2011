MY_PATH=../../..
SRC=$(MY_PATH)/SRC
SRC2=$(MY_PATH)/SRC2

TRANSLATION_PLANE_PATH=$(SRC2)/TRANSLATION_PLANE
GRASSMANN_PATH=$(SRC2)/GRASSMANN



ORDER=25
DEPTH=5
POLY=


#r10:
#	$(TRANSLATION_PLANE_PATH)/tp.out -v 5 -W -order $(ORDER) -r spread_25_10.data




starter:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 10 -starter -W -order $(ORDER) -depth $(DEPTH) -clique_level 5





