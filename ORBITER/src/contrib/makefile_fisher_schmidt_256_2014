#####################
#  PG(2,256) over F_2
#####################



matrix:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 65536 -poly_Q "65581" -q 256 -poly_q "285" -aut 1


# Aug 18, 2014 on domino 2:34:42, 24 solutions, 24 hyperovals



256_1:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 65536 -poly_Q "65581" -q 256 -poly_q "285" -aut 1 \
		-load -clique 

# Aug 18, 2014 on domino 2:18, 24 solutions, 24 hyperovals
# Aug 18, 2014 on MacBook 3:27, 24 solutions, 24 hyperovals
# Nov 24, 2014 on MacBook 3:41, 24 solutions, 24 hyperovals (takes longer because it sets up the projective_space)


256_1s:
	$(PHILIP_PATH)/fisher_schmidt_main.out -v 2 -Q 65536 -poly_Q "65581" -q 256 -poly_q "285" -aut 1 \
		-load -clique \
		-use_symmetry_group 2 
	cat FisherSchmidt_solution_256_1_case_0_* >solution_256_1.txt



# Dec 4, 2014 on MacBook 8:41 using group of order 2: 12 solutions, stored in solution_256_1.txt



