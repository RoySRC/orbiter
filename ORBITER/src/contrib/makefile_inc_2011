7_3:
	$(WIDOR_PATH)/widor.out -v 2 7_3.widor
	$(TDO_PATH)/tdo_print.out -v 2 7_3.tdo
	$(INC_GEN_PATH)/inc_gen.out -v 5 -orderly -lambda_LE  -file 7_3.tdo -sol


7_3_tda:
	$(INC_CAN_PATH)/inc_can.out -v 3 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.065mm" \
		7_3.inc



