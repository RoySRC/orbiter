MY_PATH=../../..
SRC=$(MY_PATH)/SRC
SRC2=$(MY_PATH)/SRC2

TRANSLATION_PLANE_PATH=$(SRC2)/TRANSLATION_PLANE
GRASSMANN_PATH=$(SRC2)/GRASSMANN


ORDER=27
DEPTH=5
POLY=



starter:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 10 -starter -W -order $(ORDER) -depth $(DEPTH) 

#-clique_level 5


# level 5: 50 partial spreads
# Dec 8, 2011: level 6: 10239 partial spreads in 1min 13 sec on MacBook

test:
	$(GRASSMANN_PATH)/grassmann.out -v 2 6 3 3 


search_lex:
	$(TRANSLATION_PLANE_PATH)/tp_extend.out -v 6 -order $(ORDER)  -lex $(DEPTH) 0 1  


# permutation degree is 200787
# DEPTH=5, Oct 14, 2011 on MacBook: 19 min 49 sec


search_lex_c:
	$(TRANSLATION_PLANE_PATH)/tp_extend.out -v 6 -order $(ORDER) -lex \
			-read_candidates_file spread_$(ORDER)_lvl_$(DEPTH)_candidates.txt \
			-write_graph_file \
			$(DEPTH) 0 1  

# the first one 1869 vertices with 409 solutions


# DEPTH=5, Oct 14, 2011 on MacBook: 27 Min 54 Sec


build_db:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 2 -build_db $(DEPTH) \
		-order $(ORDER) 

read:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 2 \
		-read_solution_files $(DEPTH) extend_spread_$(ORDER)_$(DEPTH)_0_1.txt -1 \
		-order $(ORDER) 

orbits:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 2 \
		-compute_orbits $(DEPTH) \
		-order $(ORDER) 

# DEPTH=5: 1176 orbits

iso:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 4 \
		-isomorph_testing $(DEPTH) \
		-order $(ORDER) 

#		-play_back event_save.txt \

print:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 4 \
		-print $(DEPTH) \
		-order $(ORDER) 

pdf:
	pdflatex spread_$(ORDER)_1.tex
	pdflatex spread_$(ORDER)_2.tex
	pdflatex spread_$(ORDER)_3.tex
	pdflatex spread_$(ORDER)_4.tex
	pdflatex spread_$(ORDER)_5.tex
	pdflatex spread_$(ORDER)_6.tex
	pdflatex spread_$(ORDER)_7.tex


