GRASSMANN_PATH=$(SRC2)/GRASSMANN


ORDER=81
DEPTH=6
POLY=




all:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 4 \
		-W -order $(ORDER) -dim_over_kernel 2


# 231 partial spreads of size 5
# 85326 partial spreads of size 6



search_lex_c:
