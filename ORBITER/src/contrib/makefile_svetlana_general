MY_PATH=~/DEV.19/GITHUB/orbiter
MY_PATH2=~/DEV.19/GITHUB/orbiter2
#MY_PATH=/scratch3/betten/COMPILE/2019_02_03
#MY_PATH2=/scratch3/betten/COMPILE/2019_02_03
SRC=$(MY_PATH)/ORBITER/src
SRC2=$(MY_PATH2)/ORBITER2/src

GRASSMANN_PATH=$(SRC2)/GRASSMANN
ALGEBRA_PATH=$(SRC)/apps/algebra
TOOLS_PATH=$(SRC)/apps/tools
COMBINATORICS_PATH=$(SRC)/apps/combinatorics
GRAPH_THEORY_PATH=$(SRC)/apps/graph_theory
PROJECTIVE_SPACE_PATH=$(SRC)/apps/projective_space
GROUPS_PATH=$(SRC)/apps/groups
GRAPH_THEORY_PATH=$(SRC)/apps/graph_theory

PACKINGS_PATH=$(SRC2)/PACKINGS
REGULAR_PACKING_PATH=$(SRC2)/REGULAR_PACKING
MATH161_PATH=$(SRC2)/MATH161



load:
	$(PROJECTIVE_SPACE_PATH)/packing_main.out -v 2 -order 16 \
		-dim_over_kernel 2 \
		-starter_size 2 \
		-input_prefix ./ \
		-solution_prefix ./ \
		-base_fname order16 \
		-load_spread_table




spread_table_compute:
	$(PROJECTIVE_SPACE_PATH)/packing_main.out -v 2 -order 16 \
		-dim_over_kernel 2 \
		-starter_size 2 \
		-input_prefix ./ \
		-solution_prefix ./ \
		-base_fname order16 \
		-compute_spread_table


O64:
	$(GROUPS_PATH)/orthogonal_group.out -v 2 -d 6 -q 4 -epsilon 1



drawO64:
	$(GRAPH_THEORY_PATH)/layered_graph_main.out -v 2 \
		-file O_1_6_4_orbit_0.layered_graph \
		-xin 1000000 -yin 1000000 \
		-xout 1000000 -yout 1000000 \
		-y_stretch 0.3 \
		-rad 2000 \
		-nodes_empty \
		-embedded \
		-spanning_tree
	pdflatex O_1_6_4_orbit_0_draw_tree.tex
	open O_1_6_4_orbit_0_draw_tree.pdf

