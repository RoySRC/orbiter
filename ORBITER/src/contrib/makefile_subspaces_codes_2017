Null:
	$(SUBSPACE_ORBITS_PATH)/subspace_orbits_main.out -v 13 \
		-n 8 -q 2 -depth 4 -null_polarity_group  \
		-draw_poset -embedded \
		-list
	pdflatex NullPolarity_8_2_poset_lvl_4.tex
	open NullPolarity_8_2_poset_lvl_4.pdf


Null_poset:
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file NullPolarity_8_2_poset_lvl_4.layered_graph \
		-draw NullPolarity_8_2_poset_lvl_4 \
		-rad 3000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-y_scale 0.5 \
		-scale 0.3 \
		-nodes_empty
	pdflatex NullPolarity_8_2_poset_lvl_4.tex
	open NullPolarity_8_2_poset_lvl_4.pdf

