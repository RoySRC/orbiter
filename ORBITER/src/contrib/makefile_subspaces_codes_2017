MY_PATH=../../../DEV.17
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2


TOOLS_PATH=$(SRC)/APPS/TOOLS
PROJECTIVE_SPACE_PATH=$(SRC)/APPS/PROJECTIVE_SPACE
SUBSPACE_ORBITS_PATH=$(SRC)/APPS/SUBSPACE_ORBITS


PG52:
	$(PROJECTIVE_SPACE_PATH)/cheat_sheet_PG.out -n 5 -q 2 
	pdflatex PG_5_2.tex
	open PG_5_2.pdf

PG72:
	$(PROJECTIVE_SPACE_PATH)/cheat_sheet_PG.out -n 7 -q 2 
	pdflatex PG_7_2.tex
	open PG_7_2.pdf


lcd:
	$(SUBSPACE_ORBITS_PATH)/subspace_orbits_main.out -v 6 \
		-depth 5 -PGL 10 2 -monomial   \
		-draw_poset -embedded \
		-list -list_LCD


so4:
	$(SUBSPACE_ORBITS_PATH)/subspace_orbits_main.out -v 13 \
		-depth 2 -PGL 4 2 -monomial -self_orthogonal  \
		-draw_poset -embedded \
		-list -table_of_nodes
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file Monomial_4_2_poset_lvl_2.layered_graph \
		-draw test \
		-rad 1000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 2000000 \
		-yout 1000000 \
		-embedded \
		-scale .24 \
		-line_width 1.0 \
		-y_stretch 0.8 \
		-spanning_tree \
		-nodes_empty
	pdflatex Monomial_4_2_poset_lvl_2_draw.tex
	open Monomial_4_2_poset_lvl_2_draw.pdf

