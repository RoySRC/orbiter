MY_PATH=../../DEV.14
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

ANURAG_PATH=$(SRC2)/ANURAG

clean:
	- rm anurag*
	- rm -rf LIFTS


starter:
	$(ANURAG_PATH)/anurag_main.out -v 5 -starter 4 ./anurag -W

lift:
	- mkdir LIFTS
	$(ANURAG_PATH)/anurag_main.out -v 10 -lift 4 ./ ./LIFTS/ ./ anurag  -solve -split 0 1000000


