MY_PATH=../../../DEV.14/
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2


HERMITIAN_PATH=$(SRC2)/HERMITIAN
SEMIFIELD_PATH=$(SRC)/SEMIFIELD
RAD=60



H_2_3:
	$(HERMITIAN_PATH)/hermitian_spread_main.out -v 2 -n 2 -Q 9 -Sbar -w

# group order 12096
# stabilizer of spread is 192
# 63 spreads


draw:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file poset_lvl_7.layered_graph \
		-draw HS \
		-xin 10000 -yin 10000 \
		-xout 800000 -yout 1100000 \
		-rad $(RAD) \
		-embedded
	pdflatex HS.tex
	open HS.pdf


#		-nodes_empty \


H_2_4:
	$(HERMITIAN_PATH)/hermitian_spread_main.out -v 2 -n 2 -Q 16 -Sbar -w


# March 20, 2014 on MacBook: 1 sec 
# 3 orbits
# (1200, 156, 100) average is 485 + 1 / 3
# depth 13 (=65/5)

# group order 249600
# number of spreads = 208, 1600, 2496


draw_4:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file poset_lvl_13.layered_graph \
		-draw HS \
		-xin 10000 -yin 10000 \
		-xout 800000 -yout 1100000 \
		-rad $(RAD) \
		-embedded
	pdflatex HS.tex
	open HS.pdf



