Q=64
GROUP=PGGL

cheat_sheet_PG2:
	$(PROJECTIVE_SPACE_PATH)/cheat_sheet_PG.out -n 2 -q $(Q) 
	pdflatex PG_2_$(Q).tex
	open PG_2_$(Q).pdf

cheat_sheet_PG3:
	$(PROJECTIVE_SPACE_PATH)/cheat_sheet_PG.out -n 3 -q $(Q) 
	pdflatex PG_3_$(Q).tex
	open PG_3_$(Q).pdf


cheat_0:
	$(SURFACE_PATH2)/cheat_sheet_surface.out -v 2 -q $(Q) -no 0
	pdflatex Surface_q$(Q)_no0.tex
	open Surface_q$(Q)_no0.pdf


Qall:
	$(SURFACE_PATH)/surface_classify.out -v 6 \
		-linear -$(GROUP) 4 $(Q) -wedge -end 


#\
#		-report


pdf:
	pdflatex Double_sixes_q$(Q).tex
	open Double_sixes_q$(Q).pdf
	pdflatex Surfaces_q$(Q).tex
	open Surfaces_q$(Q).pdf


Q:
	$(SURFACE_PATH)/surface_classify.out -v 4 -linear -$(GROUP) 4 $(Q) -wedge -end \
		-double_sixes_only
	pdflatex Double_sixes_q$(Q).tex
	open Double_sixes_q$(Q).pdf

Qr:
	$(SURFACE_PATH)/surface_classify.out -v 4 -linear -$(GROUP) 4 $(Q) -wedge -end \
		-read_double_sixes \
		-report
	pdflatex Surfaces_q$(Q).tex
	open Surfaces_q$(Q).pdf




Qrr:
	$(SURFACE_PATH)/surface_classify.out -v 4 -linear -$(GROUP) 4 $(Q) -wedge -end \
		-read_double_sixes \
		-read_surfaces \
		-recognize -q 64 -by_coefficients "1,6,1,8,1,11,1,13,1,19" -end 



