Q_draw:
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file fiveplusone_q$(Q)_poset_lvl_5.layered_graph \
		-draw test \
		-rad 8000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-scale .44 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex fiveplusone_q$(Q)_poset_lvl_5_draw.tex
	open fiveplusone_q$(Q)_poset_lvl_5_draw.pdf

#		-spanning_tree

Q_draw_detailed:
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file fiveplusone_q$(Q)_poset_detailed_lvl_5.layered_graph \
		-draw test \
		-rad 4500 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-scale .44 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex fiveplusone_q$(Q)_poset_detailed_lvl_5_draw.tex
	open fiveplusone_q$(Q)_poset_detailed_lvl_5_draw.pdf


q:
	$(OVOID_PATH)/ovoid.out -v 3 \
		-n 5 -q $(Q) -epsilon 1 \
		-surface \
		-depth 5 \
		-identify Fermat_cubic  1 0 1 1 1 2 1 3 -1 \
		-identify Clebsch_cubic 3 4 3 5 3 6 3 7 3 8 3 9 3 10 3 11 3 12 3 13 3 14 3 15 6 16 6 17 6 18 6 19 -1 \
		-identify Cayleys_nodal_cubic 1 16 1 17 1 18 1 19 -1

#nb_lines=2850 nb_pts=448 nb_orbits=14 nb_good_orbits = 5
#one with 92 points and ago=24
#another with 99 points and ago=1296
#There are 5 orbits of double sixes.
#For q=7, the cubic surface with 27 lines is equanharmonic and projectively
#unique and group of projectivities is (Z_3 x Z_3 xZ_3)S_4 and the order
#should be 648. The number of points on the surface  as you have found  is
#99, and all the points are on the lines of the surface, none of the points
#lie only one line, the number of points which is exactly on two of the 27
#lines is 81, and 18 of the points are exactly on three lines. What about
#ago=1296 in your calculation? 



iso_0:
	$(SURFACE_PATH)/iso_type_analyze.out -v 3 -linear -PGL 4 $(Q) -wedge -end \
		-no 0 \
		-apply_inv "0,4,0,0,2,6,0,0,0,6,0,2,5,1,2,5" \
		-select_five_lines 164 340 213 235 2399 \
		-print_element 7 \
		-print_element 406 \
		-print_element 6 \
		-print_element 0 \
		-subgroup "7,406,6" 24 


arcs:
	- mkdir PG_2_$(Q)
	- mkdir PG_2_$(Q)/STARTER
	- mkdir PG_2_$(Q)/SOLUTIONS
	$(ARC_PATH)/arcs_main.out -v 3 -q $(Q) -n 2 -d 2 -starter_size 6 \
		-input_prefix PG_2_$(Q)/STARTER/ \
		-solution_prefix PG_2_$(Q)/SOLUTIONS -base_fname arcs \
		-starter PG_2_$(Q)/STARTER/  \
		-W \
		-lex \
		-embedded \
		-list 6 \
		-recognize "1300,4,1,1282,157,130" \
		-recognize "260,4,1,258,161,130"


#		-recognize "2,1,0,21,46,52" \
#		-recognize "2,1,0,21,25,38"

