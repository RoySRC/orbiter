BLT_3_1:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -orthogonal 0 4 3 -save Q_4_3.inc
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 1 -q 3 -BLT_database 1
	$(INC_CAN_PATH)/inc_can.out -v 3 \
		-points_file BLT_3_1.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		Q_4_3.inc

BLT_3_1_embedded:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -PG 4 3 -save PG_4_3.inc
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 1 -q 3 -BLT_in_PG -BLT_database 1
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 1 -q 3 -orthogonal 0 -n 4 
	$(INC_CAN_PATH)/inc_can.out -v 3 \
		-points_file Q_4_3.txt \
		-points_file BLT_3_1_embedded.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		PG_4_3.inc


