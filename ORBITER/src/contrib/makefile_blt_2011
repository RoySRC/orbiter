	$(INC_CAN_PATH)/inc_can.out -v 2 -tdo -tda -anti_flag_numbers GQ_3_5_Hall.inc


# group of order 138240

Knarr_3_1_aut:
	$(GQ_PATH)/knarr.out -v 4 -q 3 -no 1 -aut




Knarr_3_1:
	$(GQ_PATH)/knarr.out -v 4 -q 3 -no 1

# writes file Knarr_GQ_3_1.inc

Knarr_3_1_group:
	$(INC_CAN_PATH)/inc_can.out -v 3 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order Knarr_GQ_3_1.inc

# group of order 26127360


Knarr_3_1_tdo:
	$(INC_CAN_PATH)/inc_can.out -v 3 \
		-rowpart 3  243 36 1 \
		-colpart 2 108 4 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order Knarr_GQ_3_1.inc

# group of order 93312


# point partition: q^5, (q+1)*q^2, 1
# line partition: (q+1)*q^3, q+1

#row_scheme:
#             |    108_{  1}     4_{  4}
#---------------------------------------
#   243_{  0} |            4           0
#    36_{  2} |            3           1
#     1_{  3} |            0           4


#col_scheme:
#             |    108_{  1}     4_{  4}
#---------------------------------------
#   243_{  0} |            9           0
#    36_{  2} |            1           9
#     1_{  3} |            0           1


Knarr_3_1_tdo1:
	$(INC_CAN_PATH)/inc_can.out -v 4 \
		-rowpart 3  243 36 1 \
		-colpart 2 108 4 \
		-lines 108 109 -1 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order Knarr_GQ_3_1.inc


#not a good choice:
#row_scheme:
#             |     54_{  1}    54_{  7}     2_{  4}     2_{  5}
#---------------------------------------------------------------
#   243_{  0} |            2           2           0           0
#    18_{  2} |            3           0           1           0
#    18_{  6} |            0           3           0           1
#     1_{  3} |            0           0           2           2
#
#col_scheme:
#             |     54_{  1}    54_{  7}     2_{  4}     2_{  5}
#---------------------------------------------------------------
#   243_{  0} |            9           9           0           0
#    18_{  2} |            1           0           9           0
#    18_{  6} |            0           1           0           9
#     1_{  3} |            0           0           1           1

# group order 15552


Knarr_3_1_tdo2:
	$(INC_CAN_PATH)/inc_can.out -v 2 \
		-rowpart 3  243 36 1 \
		-colpart 2 108 4 \
		-lines 108 111 -1 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order Knarr_GQ_3_1.inc

