a3:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -orthogonal 0 4 3 -save Q_4_3.inc
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 1 -q 3 -BLT_database 1
	$(INC_CAN_PATH)/inc_can.out -v 4 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-pic -unit_length 0.055mm\
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		Q_4_3.inc

a3b:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -orthogonal 0 4 3 -save Q_4_3.inc
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 1 -q 3 -BLT_database 1
	$(INC_CAN_PATH)/inc_can.out -v 4 \
		-points_file BLT_3_1.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-pic -unit_length 0.055mm\
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		Q_4_3.inc

a3d:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -orthogonal 0 5 3 -save Q_4_3.inc
	$(PROJECTIVE_SPACE_PATH)/make_BLT_set.out -v 1 3 1
	$(INC_CAN_PATH)/inc_can.out -v 2 \
		-distinguishedsetfile BLT_3_1.txt \
		-distinguishedpoint 18 \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme -tda -tda_files -tda_with_scheme -tda_with_group_order Q_4_3.inc


