ORDER=81
DEPTH=6
POLY=


regulus:
	$(SPREAD_PATH)/tp.out -v 5 \
		-W -order $(ORDER) -dim_over_kernel 2 \
		-extend_regulus 3 \
		-make_cases

# 2163 cases at level 3
# in 3 sec

lift:
	$(SPREAD_PATH)/tp.out -v 5 \
		-W -order $(ORDER) -dim_over_kernel 2 \
		-extend_regulus 3 \
		-lift 3 ./


