MY_PATH=../../../DEV.14/
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2


CODES_PATH=$(SRC2)/CODES
SEMIFIELD_PATH=$(SRC2)/SEMIFIELD
TREEDRAW_PATH=$(SRC2)/TREEDRAW
DRAW_PATH=$(SRC2)/DRAW




10_7_4:
	$(CODES_PATH)/codes.out -v 2 -n 10 -k 7 -q 8 -d 4 -w
 

50_45_5:
	$(CODES_PATH)/codes.out -v 4 -n 50 -k 45 -q 8 -d 5 -w

draw:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file poset_lvl_18.layered_graph \
		-draw codes \
		-xin 10000 -yin 10000 \
		-xout 800000 -yout 1100000 \
		-nodes_empty


