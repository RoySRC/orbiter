MY_PATH=~/DEV.19
SRC=$(MY_PATH)/GITHUB/orbiter/ORBITER/src
SRC2=$(MY_PATH)/ORBITER2/SRC2

PROJECTIVE_SPACE_PATH=$(SRC)/apps/projective_space
SURFACES_PATH=$(SRC)/apps/surfaces
SURFACE_PATH=$(SRC2)/SURFACE
ARCS_PATH=$(SRC)/apps/arcs




######################################################################## 
# F_8:
######################################################################## 


classify8:
	$(SURFACES_PATH)/arc_lifting_main.out -v 20 -n 2 -q 8 -classify
	pdflatex arc_lifting_q8.tex
	open arc_lifting_q8.pdf


lift:
	$(SURFACES_PATH)/arc_lifting_main.out -v 6 -n 2 -q 8 -arc "0,1,2,3,16,19"


no:
	pdflatex arc_lifting_q8.tex
	open arc_lifting_q8.pdf

