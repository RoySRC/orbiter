MY_PATH=../../SRC
MY_PATH2=../../SRC2



PG2_2:
	$(MY_PATH2)/TESTING/cheat_sheet_PG.out -v 2 -n 2 -q 2
	
PG2_4:
	$(MY_PATH2)/TESTING/cheat_sheet_PG.out -v 2 -n 2 -q 4
	
PG2_8:
	$(MY_PATH2)/TESTING/cheat_sheet_PG.out -v 2 -n 2 -q 8
	

pic:
	$(MY_PATH)/INC_CAN/inc_can.out -v 6  \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.180mm" PG_2_2.inc


analyze_PG_2_2:
	$(MY_PATH2)/PROJECTIVE_SPACE/analyze.out -v 2 -n 2 -q 2 -planes -set 0 1 2 3 4 5 6 -1 -group



arc5_tdo:
	$(MY_PATH)/INCIDENCE_STRUCTURE/tdo.out -v 2 -PG 2 8 -save PG_2_8.inc
	$(MY_PATH)/INC_CAN/inc_can.out -v 6 -distinguishedsetfile arc5.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.025mm" PG_2_8.inc

arc6_tdo:
	$(MY_PATH)/INCIDENCE_STRUCTURE/tdo.out -v 2 -PG 2 8 -save PG_2_8.inc
	$(MY_PATH)/INC_CAN/inc_can.out -v 6 -distinguishedsetfile arc6.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.025mm" PG_2_8.inc


arc6_analyze:
	$(MY_PATH2)/PROJECTIVE_SPACE/analyze.out -v 2 -n 2 -q 8 -planes -set 1 2 3 43 69 44 -1 -group


conic4_tdo:
	$(MY_PATH)/INCIDENCE_STRUCTURE/tdo.out -v 2 -PG 2 4 -save PG_2_4.inc
	$(MY_PATH)/INC_CAN/inc_can.out -v 6 -distinguishedsetfile conic_4.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.065mm" PG_2_4.inc

hyperconic4_tdo:
	$(MY_PATH)/INCIDENCE_STRUCTURE/tdo.out -v 2 -PG 2 4 -save PG_2_4.inc
	$(MY_PATH)/INC_CAN/inc_can.out -v 6 -distinguishedsetfile hyperconic_4.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.065mm" PG_2_4.inc


