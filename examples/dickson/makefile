#recognizing object 8 / 10, point 820576 lies in orbit 134
#recognizing object 9 / 10, point 1006032 lies in orbit 61


orbits_cubic_surfaces_q2_with_recognition_T:
	$(ORBITER_PATH)/orbiter.out -v 4 \
		-linear_group -PGL 4 2 \
		-end \
		-group_theoretic_activities \
		-orbits_on_polynomials 3 \
		-recognize_orbits_on_polynomials "$(TR1), $(TR2), $(TR3), $(TR4), $(TR5), $(TR6), $(TR7)" \
		-end 
	pdflatex poly_orbits_d3_n3_q2.tex
	open poly_orbits_d3_n3_q2.pdf


orbits_cubic_surfaces_q2_with_recognition_A:
	$(ORBITER_PATH)/orbiter.out -v 4 \
		-linear_group -PGL 4 2 \
		-end \
		-group_theoretic_activities \
		-orbits_on_polynomials 3 \
		-recognize_orbits_on_polynomials "$(AR1), $(AR2), $(AR3), $(AR4), $(AR5), $(AR6), $(AR7), $(AR8), $(AR9), $(AR10)" \
		-end 
	pdflatex poly_orbits_d3_n3_q2.tex
	open poly_orbits_d3_n3_q2.pdf


# investigate orbiter number 11, 15 over extension fields



Surfaces_q2:
	$(ORBITER_PATH)/orbiter.out -v 4 \
		-linear_group -PGL 4 2 \
		-end \
		-group_theoretic_activities \
		-cubic_surface_properties poly_orbits_d3_n3_q2_select.csv 2 1 \
		-end 

