Surfaces_q2_analyze:
	$(ORBITER_PATH)/orbiter.out -v 4 \
		-linear_group -PGL 4 2 \
		-end \
		-group_theoretic_activities \
		-cubic_surface_properties_analyze poly_orbits_d3_n3_q2_select_F2.csv 2 \
		-draw_options -end \
		-end 
	pdflatex poly_orbits_d3_n3_q2_select_F2_report.tex
	open poly_orbits_d3_n3_q2_select_F2_report.pdf

