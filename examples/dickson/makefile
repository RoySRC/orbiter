orbits_cubic_surfaces_q2_recognize:
	$(ORBITER_PATH)/orbiter.out -v 4 \
		-linear_group -PGL 4 2 \
		-end \
		-group_theoretic_activities \
		-orbits_on_polynomials 3 \
		-recognize_orbits_on_polynomials "$(Dickson4m1_rank), $(Dickson4m0_rank), $(Dickson6_rank), $(Dickson7_rank), $(Dickson7b_rank), $(Dickson8_rank), $(Dickson7c_rank)" 
		-end 
	pdflatex poly_orbits_d3_n3_q2.tex
	open poly_orbits_d3_n3_q2.pdf


