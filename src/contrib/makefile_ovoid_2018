clean:
	- rm log_*
	- rm graph_*
	- rm ovoid_*
	- rm -rf GRAPH_Q8 



Op42:
	$(OVOID_PATH)/ovoid.out -v 2 -epsilon 1 -n 3 -q 2

Om42:
	$(OVOID_PATH)/ovoid.out -v 2 -epsilon -1 -d 4 -q 2

Om62:
	$(OVOID_PATH)/ovoid.out -v 5 -epsilon -1 -n 5 -q 2 -draw_poset -embedded -W

# order 51840, degree 27 = U_4(2) = Weyl group of type E_6 = Sp_4(2) = O_5(3)

Om62read:
	$(OVOID_PATH)/ovoid.out -v 5 -epsilon -1 -n 5 -q 2 -read 6


drawOm62:
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file ovoid_Q-1_5_2_poset_lvl_9.layered_graph \
		-draw test \
		-rad 25000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 1000000 \
		-yout 1000000 \
		-embedded \
		-scale .44 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex ovoid_Q-1_5_2_poset_lvl_9_draw.tex
	open ovoid_Q-1_5_2_poset_lvl_9_draw.pdf


