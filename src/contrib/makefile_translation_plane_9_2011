GRASSMANN_PATH=$(SRC2)/GRASSMANN



ORDER=9
DEPTH=6
POLY=




all:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 16 -W -order $(ORDER) -T -x 1000 -y 1200


