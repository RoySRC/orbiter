test:
	$(GRASSMANN_PATH)/grassmann.out -v 2 4 2 3 


search_lex:
	$(TRANSLATION_PLANE_PATH)/tp_extend.out -v 6 \
		-order $(ORDER)  -lex \
		-read_candidates_file spread_$(ORDER)_lvl_$(DEPTH)_candidates.txt \
		-write_graph_file \
		$(DEPTH) 0 1  



