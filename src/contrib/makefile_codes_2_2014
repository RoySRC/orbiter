MY_PATH=../../../DEV.14/
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2


CODES_PATH=$(SRC2)/CODES
SEMIFIELD_PATH=$(SRC)/SEMIFIELD
TREEDRAW_PATH=$(SRC2)/TREEDRAW
DRAW_PATH=$(SRC2)/DRAW




8_4_4:
	$(CODES_PATH)/codes.out -v 2 -n 8 -k 4 -q 2 -d 4 -w
 
18_9_6:
	$(CODES_PATH)/codes.out -v 2 -n 18 -k 9 -q 2 -d 6 -w
 
23_12_7:
	$(CODES_PATH)/codes.out -v 2 -n 23 -k 12 -q 2 -d 7 -w
 
24_12_8:
	$(CODES_PATH)/codes.out -v 2 -n 24 -k 12 -q 2 -d 8 -w -draw_poset


draw:
	$(SEMIFIELD_PATH)/layered_graph_main.out -v 4 \
		-file codes_24_12_2_8_poset_lvl_24.layered_graph \
		-draw Golay24 \
		-xin 10000 -yin 10000 \
		-xout 1000000 -yout 1000000 \
		-y_stretch 1.2 \
		-nodes_empty \
		-embedded
	pdflatex Golay24.tex
	open Golay24.pdf


