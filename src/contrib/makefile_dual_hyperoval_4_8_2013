MY_PATH=../../../..
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

DUAL_HYPEROVAL_PATH=$(SRC2)/DUAL_HYPEROVAL
GRASSMANN_PATH=$(SRC2)/GRASSMANN



K=4
N=8
Q=2
DEPTH=6
POLY=



starter:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 5 -W -n $(N) -k $(K) -q $(Q)\
		-starter -depth $(DEPTH) -W


#Found 684477 orbits at depth 6
#0 : 1 orbits
#1 : 1 orbits
#2 : 1 orbits
#3 : 2 orbits
#4 : 21 orbits
#5 : 1525 orbits
#6 : 684477 orbits
#total: 686028



lift:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 5 -W -n $(N) -k $(K) -q $(Q)\
		-depth $(DEPTH) \
		-lift $(DEPTH) ./ \
		-solve -lex

lift2:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 5 -W -n $(N) -k $(K) -q $(Q)\
		-depth $(DEPTH) \
		-lift $(DEPTH) ./ \
		-solve -lex \
		-split 0 200000 



#		-memory_debug



build_db:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-build_db 



read_solutions:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-read_solution_files DH_16_depth_6_solutions.txt -1



compute_orbits:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-compute_orbits 


isomorph:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-isomorph_testing  \
		-print_interval 250



report:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-report 

magma:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-depth $(DEPTH) \
		-export_magma 


D1:
	$(DUAL_HYPEROVAL_PATH)/dh.out -v 1 -n $(N) -k $(K) -q $(Q) \
		-D1 



