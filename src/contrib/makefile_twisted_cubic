#NB_ROUNDS=4
NB_FRAMES=90
FNAME_TC=tc_%02ld_%03ld
FNAME=hilbert_%02ld_%03ld
FNAME_R0=hilbert_0_%03ld
FNAME_OLD=hilbert_%03ld
FNAME_POV=$(FNAME_TC).pov

