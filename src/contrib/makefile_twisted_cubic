		-output_mask $(FNAME_POV) \
		-nb_frames_default $(NB_FRAMES) \
		-rounds "73" \
		-W 2560 -H 1920 \
		-font_size 126 -stroke_width 5 \
		-global_picture_scale 3.0 \
		-default_angle 90 -clipping_radius 3.6 \
		-look_at "<1,1,1>*-1/sqrt(3)" \
		-zoom 73 120 70 \
		-text 73 40 "\n\n\n\n\n\nSix points in a plane\ncan be associated\nto a surface" \
		-zoom 75 18 18 \
		-clipping 75 0.7 \
	date
	cp PICS/* POV
	mv *.pov POV
	mv makefile_animation POV

