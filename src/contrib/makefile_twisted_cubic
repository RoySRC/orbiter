create41:
	- rm -rf POV
	mkdir POV
	$(SURFACE_PATH)/animate.out -v 2 -Hilbert \
		-output_mask $(FNAME_POV) \
		-nb_frames_default $(NB_FRAMES) \
		-rounds "41" \
		-W 2560 -H 1920 \
		-font_size 126 -stroke_width 5 \
		-global_picture_scale 3.0 \
		-angle 24 -clipping_radius 2.8 \
		-zoom 41 120 120  \
		-no_background 41 \
		-no_bottom_plane 41 \
	date
	cp PICS/* POV
	mv *.pov POV
	mv makefile_animation POV


