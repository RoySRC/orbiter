extra:
	- rm -rf POV
	mkdir POV
	$(SURFACE_PATH)/animate.out -v 2 -Hilbert \
		-output_mask $(FNAME_POV) \
		-nb_frames_default $(NB_FRAMES) \
		-rounds "2" \
		-W 800 -H 600 \
		-font_size 36 -stroke_width 2 \
		-global_picture_scale 1.0 \
		-angle 24 -clipping_radius 1.8 \
		-zoom 61 24 46 \
		-zoom 62 46 46 \
		-zoom 63 48 72 \
		-zoom 64 72 72 \
		-zoom 65 72 96 \
		-zoom 66 48 48 \
		-text 66 40 "\n\n\n\n\n\nA trihedron\n(plus the plane at infinity)" \
		-zoom 67 48 48 \
		-text 67 40 "\n\n\n\n\n\nNine lines\n(three at infinity)" \
		-zoom 68 48 48 \
		-text 68 40 "\n\n\n\n\n\nAnother trihedron" \
		-zoom 69 48 48 \
		-text 69 40 "\n\n\n\n\n\nThey share the same nine lines" \
		-picture 70 0.3 "Jacob_Steiner.png" "-gravity NorthWest -geometry +20+20" \
		-zoom 70 48 48 \
		-text 70 40 "\n\n\n\n\n\nA Steiner trihedral pair" \
		-zoom 71 48 48 \
		-zoom 73 18 18 \
		-clipping 73 0.7 \
		-text 73 40 "\n\n\n\n\n\nThere are 6 base points" \
		-zoom 75 18 18 \
		-clipping 75 0.7 \
		-text 17 40 "\n\n\n\n\n\nIt has Tetrahedral\nsymmetry Alt(4)" \


#		-text 50 40 "Hence the name\nruled surface" \
#		-text 10 40 "Following Hilbert\nand Cohn-Vossen (1932),\na cubic surface\nis constructed" \

no:
		-text 3 40 "" \
		-text 30 40 "Question:\n\nWhat is the minimal\n\ndata which determines\n\na surface?" \
		-text 31 40 "Answer:\n\nA) a five-plus-one\n   of lines\n\nB) a six arc\n   not on a conic" \
		-text 16 40 "A five-plus-one:\n\nfive lines (blue)\nwith a common\ntransversal (red)" \
		-text 32 40 "A six arc\nnot on a conic" \
		-text 3 30 "Consider three\nof the blue lines" \
		-text 4 30 "They define a hyperboloid" \
		-text 5 30 "Any further blue line\nis a secant" \
		-text 6 30 "The second point\nof intersection\ndefines another\nred line" \
		-text 7 30 "That way, \nall 12 lines\nof the double six\ncan be found" \
		-text 19 40 "The plane z=0" \
		-text 18 40 "The plane pi_{12,34,56}" \
		-text 22 40 "The six base points\nof the Clebsch map" \



big:
	povray -W1920 -H1200 hilbert_55_053.pov


not:
		-text 27 40 "Two lines ai and bi are paired\n\n\nif they are disjoint\n" \
		-text 28 40 "Two lines ai and bi are paired\n\n\nif they are orthogonal\n" \
		-text 29 40 "For instance,\n\na1 is paired with b1,\n\nthey are orthogonal" \





# -angle 18 is narrower, and closer
# -angle 38 is wider, and further away

r7:
	$(SURFACE_PATH)/animate.out -v 2 -n $(N) \
		-output_mask $(FNAME_POV) \
		-nb_frames $(NB_FRAMES) \
		-rounds 3 7 \
		-clebsch_up_and_down 20 17 22 24 \
		-angle 28 -clipping_radius 3.6



#		-i 0 $(NB_FRAMES) HIGHRES/hilbert_17_%03ld.png \
# it has tetrahedral symmetry

# 16 is five plus one of lines
# 32 is 6 arc in plane
# 3 is 3 skew blue lines
# 4 is hyperboloid and 3 skew blue lines
# 5 is hyperboloid plus 3 blue plus one red plus one blue secant
# 6 is two red lines 4 blue lines
# 7 is two red lines 4 blue lines
# 19 is surface plus plane z=0
# 18 is tritangent plane pi_12,34,56
# 22 is surface + double six + tritangent plane pi_12,34,56 + balls appearing
# 23 is surface + double six + tritangent plane pi_12,34,56 + balls 
# 29 is cube frame plus a1 plus b1


