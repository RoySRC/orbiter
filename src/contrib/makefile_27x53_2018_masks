MY_PATH=~/DEV.18/GITHUB/orbiter/ORBITER/
#MY_PATH=/scratch3/betten/COMPILE/2018_08_14/ORBITER/
SRC=$(MY_PATH)/src
SRC2=$(MY_PATH)/ORBITER2/SRC2

GROUPS_PATH=$(SRC)/apps/groups
TOOLS_PATH=$(SRC)/apps/tools



all:
	$(GROUPS_PATH)/direct_product.out -v 5 -d1 6 -q1 1 -d2 6 -q2 1 \
		-K 6 \
		-DDx 2 \
		-DDy 1 \
		-depth 6 -W



draw:
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file design_no_group_6_6_poset_lvl_6.layered_graph \
		-draw test \
		-rad 15000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 2000000 \
		-yout 1000000 \
		-embedded \
		-scale .27 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex design_no_group_6_6_poset_lvl_6_draw.tex
	open design_no_group_6_6_poset_lvl_6_draw.pdf


draw_tree:
	$(TOOLS_PATH)/layered_graph_main.out -v 4 \
		-file design_no_group_6_6_tree_lvl_6.layered_graph \
		-draw test \
		-rad 15000 \
		-xin 1000000 \
		-yin 1000000 \
		-xout 2000000 \
		-yout 1000000 \
		-embedded \
		-scale .27 \
		-line_width 1.0 \
		-y_stretch 0.8 
	pdflatex design_no_group_6_6_tree_lvl_6_draw.tex
	open design_no_group_6_6_tree_lvl_6_draw.pdf


