CaseAB:
	$(THE_PATH)/tdo_start.out -linearspace 30 7 1 5 15 4 44 -1 Line6
	$(THE_PATH)/tdo_refine.out -v 3 Line6.tdo >log6.1
	$(THE_PATH)/tdo_refine.out -v 4  1 Line6r.tdo >log6.2

# this gives the 171 cases:
171:
	$(THE_PATH)/tdo_start.out -linearspace 30 7 1 5 15 4 44 -1 Line6
	$(THE_PATH)/tdo_refine.out -v 3 Line6.tdo >log6.1
	# compute the next column tactical decomposition, 
	# ignore the 44 4-lines in the distribution (2nd system):
	$(THE_PATH)/tdo_refine.out -v 5 -o2 1 Line6r.tdo >log6.2
	# compute the next row tactical decomposition, 
	# ignore the 44 4-lines in the types (1st system):
	$(THE_PATH)/tdo_refine.out -v 5 -o1 1 Line6rr.tdo >log6.3


