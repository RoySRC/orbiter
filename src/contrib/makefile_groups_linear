O62:
	$(GROUPS_PATH)/linear_group.out -v 2  \
		-linear -PGL 6 2 -orthogonal -1 -end \

#Strong generators for a group of order 51840:


Om62:
	$(GROUPS_PATH)/orthogonal_group.out -v 2 -epsilon -1 -d 6 -q 2

#order_PO_minus the order of PO^-(6,2) is 51840
#order_PO_epsilon  f_semilinear=0 epsilon=-1 k=5 q=2 order=51840
# Not true: order 25920, degree 27 = U_4(2) = Weyl group of type E_6 = Sp_4(2) = O_5(3)


tree:
	$(GRAPH_THEORY_PATH)/layered_graph_main.out -v 2 \
		-file O_-1_6_2_orbit_0.layered_graph \
		-xin 1000000 -yin 1000000 \
		-xout 1000000 -yout 1000000 \
		-y_stretch 0.3 \
		-rad 4000 \
		-nodes_empty \
		-embedded \
		-spanning_tree
	pdflatex O_-1_6_2_orbit_0_draw_tree.tex
	open O_-1_6_2_orbit_0_draw_tree.pdf



