MY_PATH=../../..
SRC=$(MY_PATH)/SRC
SRC2=$(MY_PATH)/SRC2

TRANSLATION_PLANE_PATH=$(SRC2)/TRANSLATION_PLANE
GRASSMANN_PATH=$(SRC2)/GRASSMANN
DIOPHANT_PATH=$(SRC)/DIOPHANT


ORDER=64
DEPTH=4
POLY=


starter:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 4 \
		-W -order $(ORDER) \
		-depth $(DEPTH) \
		-dim_over_kernel 2 \
		-starter

# 7 orbits on 4 sets of lines in PG(3,8)
# distribution of ago: (6048, 648, 96, 84, 72, 56, 36)

# {0, 4744, 81, 162} with stabilizer of order 6048.
# {0, 4744, 81, 593} with stabilizer of order 648.
# {0, 4744, 81, 594} with stabilizer of order 72.




seventytwo:
	$(TRANSLATION_PLANE_PATH)/tp.out -v 5 \
		-W -order $(ORDER) -dim_over_kernel 2 \
		-extend_regulus 3 \
		-make72 spread_64_lvl_4 4


