canon8_0:
	$(PROJECTIVE_SPACE_PATH)/canonical_form.out -v 10 -n 3 -q 8 \
		-input -file_of_points surface_catalogue_q8_0_points.txt -end \
		-classify_nauty \
		-prefix surface_catalogue_q8_0_points \
		-save surface_catalogue_q8_0_points \
		-latex 
	pdflatex surface_catalogue_q8_0_points_classification.tex
	open surface_catalogue_q8_0_points_classification.pdf


try:
	$(SURFACES_PATH)/create_surface_main.out -v 2 \
		-description -q 8 -by_coefficients "1,6,1,8,1,11,1,13,1,19" -end 

try2:
	$(SURFACES_PATH)/surface_classify.out -v 2 \
		-linear -PGGL 4 8 -wedge -end \
		-recognize -q 8 -by_coefficients "1,6,1,8,1,11,1,13,1,19" -end 

