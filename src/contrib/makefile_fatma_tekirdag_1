try:
	$(SURFACES_PATH)/create_surface_main.out -v 2 \
		-description -q 8 -by_coefficients "1,6,1,8,1,11,1,13,1,19" -end 

try2:
	$(SURFACES_PATH)/surface_classify.out -v 2 \
		-linear -PGGL 4 8 -wedge -end \
		-recognize -q 8 -by_coefficients "1,6,1,8,1,11,1,13,1,19" -end 

