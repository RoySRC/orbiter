arcs:
	- mkdir PG_2_$(Q)
	- mkdir PG_2_$(Q)/STARTER
	- mkdir PG_2_$(Q)/SOLUTIONS
	$(ARCS_PATH)/arcs_main.out -v 3 -q $(Q) -n 2 -d 2 -starter_size 6 \
		-input_prefix PG_2_$(Q)/STARTER/ \
		-solution_prefix PG_2_$(Q)/SOLUTIONS -base_fname arcs \
		-starter PG_2_$(Q)/STARTER/  \
		-W \
		-lex \
		-embedded \
		-draw_poset \
		-list 6 \
		-Report \
		-tools_path $(GRAPH_THEORY_PATH) \
		-export_schreier_trees \
		-report_schreier_trees
	cp PG_2_8/STARTER/arcs_poset_detailed_lvl_6.tex arcs_poset_detailed_lvl_6.tex
	latex Arcs_q8.tex
	dvips Arcs_q8.dvi -o
	open Arcs_q8.ps


#	pdflatex arcs_poset_detailed_lvl_6.tex
#	open arcs_poset_detailed_lvl_6.pdf

#		-recognize "1300,4,1,1282,157,130" \
#		-recognize "260,4,1,258,161,130"

lift:
	$(SURFACES_PATH)/create_surface_main.out -v 2 \
		-description -q 8 -arc_lifting "0,1,2,3,28,35" -end


