MY_PATH=~/DEV.18/GITHUB/orbiter
MY_PATH2=~/DEV.18
#MY_PATH=/scratch3/betten/COMPILE/2018_04_17
ORBITER=$(MY_PATH)/ORBITER
ORBITER2=$(MY_PATH2)/ORBITER2
SRC=$(ORBITER)/SRC
SRC2=$(ORBITER2)/SRC2


ARCS_PATH=$(SRC)/APPS/ARCS
TOOLS_PATH=$(SRC)/APPS/TOOLS
PROJECTIVE_SPACE_PATH=$(SRC)/APPS/PROJECTIVE_SPACE
KRAMER_MESNER_PATH=$(SRC2)/KRAMER_MESNER

q4:
	$(ARCS_PATH)/arcs_orderly.out -v 2 -k 2 -q 2 -sz 4

q8:
	$(ARCS_PATH)/arcs_orderly.out -v 2 -k 2 -q 8 -sz 10


vq8:
	valgrind --tool=callgrind $(ARCS_PATH)/arcs_orderly.out -v 2 -k 2 -q 8 -sz 10

vq8s:
	qcachegrind callgrind.out.93612
	callgrind_annotate callgrind.out.93612

q16:
	$(ARCS_PATH)/arcs_orderly.out -v 1 -k 2 -q 16 -sz 18


# Sept 3, 2018: 50 sec on Mac



vq16:
	valgrind --tool=callgrind $(ARCS_PATH)/arcs_orderly.out -v 1 -k 2 -q 16 -sz 18

vq16s:
	qcachegrind callgrind.out.85160
	callgrind_annotate callgrind.out.85160


