MY_PATH=../../../DEV.13
#MY_PATH=/scratch/betten/COMPILE/2013_01_23
SRC=$(MY_PATH)/ORBITER/SRC
SRC2=$(MY_PATH)/ORBITER2/SRC2

REGULAR_PACKING_PATH=$(SRC2)/REGULAR_PACKING
TRANSLATION_PLANE_PATH=$(SRC2)/TRANSLATION_PLANE
GRASSMANN_PATH=$(SRC2)/GRASSMANN


Q=2
K=2
NO=0
POLY=
DEPTH=2

klein:
	$(TRANSLATION_PLANE_PATH)/klein.out -v 2 -q $(Q)

RP:
	$(REGULAR_PACKING_PATH)/regular_packing_main.out -v 2 -q $(Q) -k $(K) -no $(NO) 

starter:
	$(REGULAR_PACKING_PATH)/regular_packing_main.out -v 5 -q $(Q) -k $(K) -no $(NO) -starter $(DEPTH) 

lift:
	$(REGULAR_PACKING_PATH)/regular_packing_main.out -v 5 -q $(Q) -k $(K) -no $(NO) -lift $(DEPTH) "./" -solve  


#-lex
#-dimension 4

analyze:
	$(REGULAR_PACKING_PATH)/regular_packing_main.out -v 5 -q $(Q) -k $(K) -no $(NO) -analyze solution_1.txt


#Decomposition scheme:
#\begin{array}{r|*{3}{r}}
#\rightarrow  &     21 &     28 &      7\\
#\hline
#     7 &            6 &            0 &            0\\
#    21 &            1 &            4 &            1\\
#    35 &            0 &            0 &            0\\
#\end{array}
#\begin{array}{r|*{3}{r}}
#\downarrow  &     21 &     28 &      7\\
#\hline
#     7 &            2 &            0 &            0\\
#    21 &            1 &            3 &            3\\
#    35 &            0 &            0 &            0\\
#\end{array}
#regular_packing::analyze_packing done


identify_subsets:
	$(REGULAR_PACKING_PATH)/regular_packing_main.out -v 2 -q $(Q) -k $(K) -no $(NO) -identify_subsets solution_1.txt STARTER_REGULAR_SPREADS/partial_packings_2_2_ $(DEPTH)

stabilizer:
	$(REGULAR_PACKING_PATH)/regular_packing_main.out -v 2 -q $(Q) -k $(K) -no $(NO) -stabilizer solution_1.txt STARTER_REGULAR_SPREADS/partial_packings_2_2_ $(DEPTH) identify_subsets.csv 0

# group order 3



