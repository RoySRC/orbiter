a:
	$(ALGEBRA_PATH)/eigenstuff.out -q 13 -M 4 "1,0,0,0,0,0,12,0,0,1,1,0,6,3,10,12"


13_0:
	$(SURFACES_PATH)/create_surface_main.out -v 2 \
		-description -q 13 -catalogue 0 -end
	pdflatex surface_catalogue_q13_0.tex
	open surface_catalogue_q13_0.pdf
b:
	$(ALGEBRA_PATH)/eigenstuff.out -q 13 -M 4 "1,7,11,0,2,1,0,11,8,5,0,0,1,5,0,0"


13_0t:
	$(SURFACES_PATH)/create_surface_main.out -v 2 \
		-description -q 13 -catalogue 0 -end \
		-transform_inverse "3,1,10,12,7,10,11,12,8,4,11,12,5,12,5,12"
	pdflatex surface_catalogue_q13_0.tex
	open surface_catalogue_q13_0.pdf

