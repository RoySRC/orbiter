	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 2 -q 4 -n 3 \
		-twisted_cubic 


# twisted_cubic_4.txt


twisted_cubic_c:
	$(PROJECTIVE_SPACE_PATH)/canonical_form.out -v 2 \
		-q 4 -n 3 \
		-input -file_of_point_set \
			Twisted_cubic_q4.txt -end \
		-classify_nauty \
		-prefix twisted_cubic -latex
	pdflatex twisted_cubic_classification.tex
	open twisted_cubic_classification.pdf


