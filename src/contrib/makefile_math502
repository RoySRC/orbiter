singer23_order2:
	$(GROUPS_PATH)/linear_group.out -v 4  \
		-linear -PSL 2 23 -singer 22 -end \
		-normalizer

# creates a file Singer_2_23_power22_normalizer.magma
# running this through magma creates a file 
# Singer_2_23_power22_normalizer.txt
#
# creates the Dihedral group of order 24.
# creates normalizer_24.tex

singer23_tex:
	$(TOOLS2_PATH)/a2tex.out <PGL_2_23_Singer_2_23_power22_normalizer.magma >PGL_2_23_Singer_2_23_power22_normalizer.magma.tex
	$(TOOLS2_PATH)/a2tex.out <PGL_2_23_Singer_2_23_power22_normalizer.txt >PGL_2_23_Singer_2_23_power22_normalizer.txt.tex
	$(TOOLS2_PATH)/a2tex.out <PGL_2_23_Subgroup_Z22_4_normalizer.magma  >PGL_2_23_Subgroup_Z22_4_normalizer.magma.tex
	$(TOOLS2_PATH)/a2tex.out <PGL_2_23_Subgroup_Z22_4_normalizer.txt  >PGL_2_23_Subgroup_Z22_4_normalizer.txt.tex



# the subgroup Z_2 x Z_2 is generated by a^6 and b with
# a^6 = 
#  1  1
# 18 22
#
# b =
#  1 14
# 22 22


singer23_Z22:
	$(GROUPS_PATH)/linear_group.out -v 2  \
		-linear -PSL 2 23 -subgroup_by_generators \
			Z22 4 2 "1,1,18,22" "1,14,22,22" -end \
		-normalizer
	#pdflatex normalizer_24.tex

# Coxeter generators:
# s1 = Elt 2 = 1,9,20,22
# s2 = Elt 6 = 1,7,2,22
# s3 = Elt 3 = 1,19,1,22



singer23_sym4:
	$(GROUPS_PATH)/linear_group.out -v 2  \
		-linear -PSL 2 23 -subgroup_by_generators \
			SYM4 24 3 "1,9,20,22" "1,7,2,22" "1,19,1,22" -end \
		-print_elements_tex


O4_23:
	$(GROUPS_PATH)/exceptional_isomorphism_O4_main.out -v 2 -q 23 \
		-2to4 3 \
		"1,9,20,22" "1,0,0,1" 0 \
		"1,7,2,22" "1,0,0,1" 0 \
		"1,19,1,22" "1,0,0,1" 0

O4_23_sym4:
	$(GROUPS_PATH)/linear_group.out -v 2  \
		-linear -PGL 4 23 \
		-subgroup_by_generators Sym4 24 3 \
			" 1,  0,  0,  9,  0, 22,  3,  0,  0, 14,  1,  0, 20,  0,  0, 22, " \
			" 1,  0,  0,  7,  0, 22, 21,  0,  0, 16,  1,  0,  2,  0,  0, 22, " \
			" 1,  0,  0, 19,  0, 22, 22,  0,  0,  4,  1,  0,  1,  0,  0, 22, " \
		-end \
		-print_elements_tex
	pdflatex PGL_4_23_Subgroup_Sym4_24_elements.tex
	open PGL_4_23_Subgroup_Sym4_24_elements.pdf


O4to5:
