Fermat_curve:
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 2 -q 4 -n 2 \
		-projective_variety "Fermat_curve_q4" 3 "1,0,1,1,1,2"

#x^3+y^3+z^3


#0 & X^3 & ( 3, 0, 0 )\\
#1 & Y^3 & ( 0, 3, 0 )\\
#2 & Z^3 & ( 0, 0, 3 )\\
#3 & X^2Y & ( 2, 1, 0 )\\
#4 & X^2Z & ( 2, 0, 1 )\\
#5 & XY^2 & ( 1, 2, 0 )\\
#6 & Y^2Z & ( 0, 2, 1 )\\
#7 & XZ^2 & ( 1, 0, 2 )\\
#8 & YZ^2 & ( 0, 1, 2 )\\
#9 & XYZ & ( 1, 1, 1 )\\


