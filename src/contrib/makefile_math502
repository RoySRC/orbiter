#NB_ROUNDS=4
NB_FRAMES=30
FNAME=tc_%02ld_%03ld
FNAME_POV=$(FNAME).pov

# either:

#		-W 800 -H 600 \
#		-font_size 36 -stroke_width 2 \
#		-global_picture_scale 1.0 \

# or:

#		-W 2560 -H 1920 \
#		-font_size 126 -stroke_width 5 \
#		-global_picture_scale 3.0 \



tc:
	- rm -rf POV
	mkdir POV
	$(SRC2_SURFACE_PATH)/animate.out -v 2 -twisted_cubic \
		-output_mask $(FNAME_POV) \
		-rounds "0,1" \
		-nb_frames_default 90 \
		-video_options \
		-default_angle 50 -clipping_radius 1.8 \
		-camera 0 "<1,1,1>" "<-3,1,3>" "<1,1,1>*1/2" \
		-camera 1 "<1,1,1>" "<-3,1,3>" "<1,1,1>*1/2" \
		-W 2560 -H 1920 \
		-end
	mv *.pov POV
	mv makefile_animation POV


#		-no_bottom_plane 0 \
#		-camera 0 "<1,1,1>" "<-40+20,1+20,40+20>" "<25,25,25>" \
#		-zoom 0 145 145 \
#		-W 800 -H 600 \
#		-W 2560 -H 1920 \


tcv:
	- rm tc.mp4
	- rm -r FRAMES_TC
	- mkdir FRAMES_TC
	$(TOOLS_PATH)/prepare_frames.out \
		-i 0 90 PNG0/tc_00_%03ld.png \
		-i 1 90 PNG0/tc_01_%03ld.png \
		-output_starts_at 0 \
		-o FRAMES_TC/frame%04d.png
	ffmpeg -r 15 -f image2 -i FRAMES_TC/frame%04d.png -f mp4 -q:v 0 -vcodec mpeg4  tc.mp4



