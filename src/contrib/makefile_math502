	$(GROUPS_PATH)/linear_group.out -v 2  \
		-linear -PGL 5 23 \
		-subgroup_by_generators Sym4 24 3 \
			" 1,  0,  0,  0,  0,  0, 13,  0,  0,  2,  0,  0, 10, 16,  0,  0,  0, 21, 13,  0,  0,  7,  0,  0, 10, " \
			" 1,  0,  0,  0,  0,  0,  7,  0,  0,  3,  0,  0, 16,  9,  0,  0,  0, 20,  7,  0,  0, 14,  0,  0, 16, " \
			" 1,  0,  0,  0,  0,  0, 13,  0,  0, 17,  0,  0, 10, 10,  0,  0,  0,  6, 13,  0,  0, 13,  0,  0, 10, " \
		-end \
		-print_elements_tex


BLT23_sym4:
	$(GROUPS_PATH)/linear_group.out -v 2  \
		-linear -PGL 5 23 \
		-subgroup_by_generators Sym4 24 3 \
			"19,0,0,11,7, 0,0,3,0,0, 0,8,0,0,0, 15,0,0,14,2, 17,0,0,4,14" \
			"16,9,21,17,5, 22,13,8,12,20, 16,12,13,1,2, 14,2,20,13,19, 20,1,12,9,13" \
			"20,19,1,16,17, 0,6,8,17,8, 16,16,4,5,5, 22,22,2,4,6, 2,8,4,3,11" \
		-end \
		-print_elements_tex
	pdflatex Subgroup_Sym4_24_elements.tex
