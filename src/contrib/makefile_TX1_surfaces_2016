Q13:
	$(SURFACE_PATH)/surface.out -v 3 -linear -PGL 4 13 -wedge -end \
		-identify Fermat_cubic  1 0 1 1 1 2 1 3 -1 \
		-identify Clebsch_cubic 3 4 3 5 3 6 3 7 3 8 3 9 3 10 3 11 3 12 3 13 3 14 3 15 6 16 6 17 6 18 6 19 -1 \
		-identify Cayleys_nodal_cubic 1 16 1 17 1 18 1 19 -1 \
		-identify E4 7 4 2 7 6 12 6 15 12 16 3 17 -1 \
		-identify E6t1 12 4 12 7 9 12 10 15 10 16 6 17 -1 \
		-identify E6t2 9 4 1 7 5 12 9 15 9 16 6 17 11 18 9 19 -1 \
		-identify E9 6 4 2 7 11 12 10 15 5 16 7 17 -1 \
		-identify E18 1 0 1 1 1 2 1 3 -1 



q13:
	$(OVOID_PATH)/ovoid.out -v 3 \
		-n 5 -q 13 -epsilon 1 \
		-surface \
		-depth 5 \
		-identify Fermat_cubic  1 0 1 1 1 2 1 3 -1 \
		-identify Clebsch_cubic 3 4 3 5 3 6 3 7 3 8 3 9 3 10 3 11 3 12 3 13 3 14 3 15 6 16 6 17 6 18 6 19 -1 \
		-identify Cayleys_nodal_cubic 1 16 1 17 1 18 1 19 -1 \
		-identify E4 7 4 2 7 6 12 6 15 12 16 3 17 -1 \
		-identify E6t1 12 4 12 7 9 12 10 15 10 16 6 17 -1 \
		-identify E6t2 9 4 1 7 5 12 9 15 9 16 6 17 11 18 9 19 -1 \
		-identify E9 6 4 2 7 11 12 10 15 5 16 7 17 -1 \
		-identify E18 1 0 1 1 1 2 1 3 -1 

