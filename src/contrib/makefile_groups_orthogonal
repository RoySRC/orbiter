MY_PATH=~/DEV.19/GITHUB/orbiter
#MY_PATH=/scratch3/betten/COMPILE/2018_10_07/
SRC=$(MY_PATH)/ORBITER/src
SRC2=$(MY_PATH)/ORBITER2/src2

GROUPS_PATH=$(SRC)/apps/groups
TOOLS_PATH=$(SRC)/apps/tools





O53:
	$(GROUPS_PATH)/orthogonal_group.out -v 2 -epsilon 0 -d 5 -q 3

O5_23:
	$(GROUPS_PATH)/orthogonal_group.out -v 2 -epsilon 0 -d 5 -q 23

draw5_23:
	$(TOOLS_PATH)/layered_graph_main.out -v 2 \
		-file O_0_5_23_orbit_0.layered_graph \
		-xin 1000000 -yin 1000000 \
		-xout 1000000 -yout 1000000 \
		-y_stretch 0.3 \
		-rad 2000 \
		-nodes_empty \
		-embedded \
		-spanning_tree
	pdflatex O_0_5_23_orbit_0_draw_tree.tex
	open O_0_5_23_orbit_0_draw_tree.pdf


O5_97:
	$(GROUPS_PATH)/orthogonal_group.out -v 2 -epsilon 0 -d 5 -q 97


