	$(GRAPH_PATH)/graph.out -n 5 -v 2 -tournament

t6:
	$(GRAPH_PATH)/graph.out -n 6 -v 2 -tournament

t7:
	$(GRAPH_PATH)/graph.out -n 7 -v 2 -tournament


g4:
	$(GRAPH_PATH)/graph.out -n 4 -v 3

g5:
	$(GRAPH_PATH)/graph.out -n 5 -v 2

g6:
	$(GRAPH_PATH)/graph.out -n 6 -v 2


g7:
	$(GRAPH_PATH)/graph.out -n 7 -v 2

