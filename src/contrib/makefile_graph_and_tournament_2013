MY_PATH=../..
SRC=$(MY_PATH)/SRC


GRAPH_PATH=$(SRC)/GRAPH




t3:
	$(GRAPH_PATH)/graph.out -n 3 -v 2 -tournament 

t4:
	$(GRAPH_PATH)/graph.out -n 4 -v 2 -tournament -W

