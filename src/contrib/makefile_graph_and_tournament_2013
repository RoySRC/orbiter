g9:
	$(GRAPH_PATH)/graph.out -n 9 -v 2

# 274668 graphs total in 4:54 on MacBook Aug 3, 2011



# regular graphs:

g6r3:
	$(GRAPH_PATH)/graph.out -n 6 -depth 9 -regular 3 -v 2

g8r3:
	$(GRAPH_PATH)/graph.out -n 8 -depth 12 -regular 3 -v 2 -W

# 6 graphs, ago = (1152, 48, 16^2, 12, 4)

