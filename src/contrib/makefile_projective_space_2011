	$(PROJECTIVE_SPACE_PATH)/make_something.out -q 8 -hyperoval 
	$(INC_CAN_PATH)/inc_can.out -v 3 -points_file hyperoval_regular_q8.txt \
		-save_stabilizer \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.035mm" \
		-save_stabilizer \
		PG_2_8.inc
	mv PG_2_8_0_stabilizer.txt hyperoval_regular_q8_stabilizer.txt
	$(INC_CAN_PATH)/read_group.out -v 2 -nb_rows 73 -nb_cols 73 -PG 2 8 \
		-file hyperoval_regular_q8_stabilizer.txt \
		-save 
	$(OYA_PATH)/arc.out -v 2 -q 8   \
		-stab hyperoval_regular_q8_stabilizer_linear.txt \
		-set hyperoval_regular_q8.txt \
		-W -depth 4
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -n 2 -q 8 \
		-file hyperoval_regular_q8.txt \
		-test_diagonals PGGL_3_8_lvl_4


