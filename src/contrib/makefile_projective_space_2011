# observe: \ell_0 is in C_6, which is an orbit of length 1.
# hence the line stabilizer of \ell_0 has index 1, i.e. is a group of order 768



andre_BM4_and_spread_moved:
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 1 -q 4 \
		-Buekenhout_Metz 
	$(PROJECTIVE_SPACE_PATH)/process.out -v 3 -n 2 -q 16 -move_line 2 0 -file unital_BM4.txt 
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -PG 2 16 -save PG_2_16.inc
	$(INC_CAN_PATH)/inc_can.out -v 3  -save_stabilizer \
		-points_file  unital_BM4_moved_line.txt  \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme -tdo_write_class_files \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.010mm" \
		PG_2_16.inc
	# now, \ell_0 is in TDA-class 6, which is an orbit of length 64
	$(PROJECTIVE_SPACE_PATH)/process.out -v 3 -q 4 -andre -file unital_BM4_moved_line.txt 
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -PG 4 4 -save PG_4_4.inc
	$(INC_CAN_PATH)/inc_can.out -v 3  -save_stabilizer \
		-points_file  unital_BM4_moved_line_andre.txt  \
		-lines_file desarguesian_line_spread_in_PG_3_4_embedded.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme -tdo_write_class_files \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.010mm" \
		PG_4_4.inc


