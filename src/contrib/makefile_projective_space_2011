LS:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -poly 19  -PG 2 16 -save PG_2_16.inc
	$(INC_CAN_PATH)/inc_can.out -v 3 -points_file LunelliSce.txt \
		-save_stabilizer \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.035mm" \
		-save_stabilizer \
		PG_2_16.inc
	mv PG_2_16_0_stabilizer.txt LunelliSce_stabilizer.txt
	$(INC_CAN_PATH)/read_group.out -poly 19 -v 2 -nb_rows 273 -nb_cols 273 -PG 2 16 \
		-file LunelliSce_stabilizer.txt \
		-save 
	$(OYA_PATH)/arc.out -v 2 -q 16 -poly 19  \
		-stab LunelliSce_stabilizer_linear.txt \
		-set LunelliSce.txt \
		-W -depth 4
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -n 2 -q 16 -poly 19 \
		-file LunelliSce.txt \
		-test_diagonals PGGL_3_16_lvl_4

# some orbits work, some don't


# group order 144

LS_subarcs:
	$(OYA_PATH)/arc.out -v 2 -q 16 -poly 19  \
		-stab LunelliSce_stabilizer_linear.txt \
		-set LunelliSce.txt \
		-W -depth 4

# 27 orbits on quadrangles



