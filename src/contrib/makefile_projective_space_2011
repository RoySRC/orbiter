c3:
	$(PROJECTIVE_SPACE_PATH)/make_something.out -v 2 -q 3 -twisted_cubic
	$(PROJECTIVE_SPACE_PATH)/analyze.out -v 2 -n 3 -q 3 -planes -file twisted_cubic_3.txt -group
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -PG 3 3 -save PG_3_3.inc
	$(INC_CAN_PATH)/inc_can.out -v 6 -points_file twisted_cubic_3.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.035mm" \
		PG_3_3.inc
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -q 3 -n 3 \
		-group -file twisted_cubic_3.txt


# group order 192



