H4_quadrangles:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2  -PG 2 4 -save PG_2_4.inc
	$(PROJECTIVE_SPACE_PATH)/make_something.out -q 4 -hyperoval 
	$(INC_CAN_PATH)/inc_can.out -v 3 -points_file hyperoval_regular_q4.txt \
		-save_stabilizer \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.035mm" \
		-save_stabilizer \
		PG_2_4.inc
	mv PG_2_4_0_stabilizer.txt hyperoval_regular_q4_stabilizer.txt
	$(INC_CAN_PATH)/read_group.out -v 2 -nb_rows 21 -nb_cols 21 -PG 2 4 \
		-file hyperoval_regular_q4_stabilizer.txt \
		-save 
	$(OYA_PATH)/arc.out -v 2 -q 4   \
		-stab hyperoval_regular_q4_stabilizer_linear.txt \
		-set hyperoval_regular_q4.txt \
		-W -depth 4
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -n 2 -q 4 \
		-file hyperoval_regular_q4.txt \
		-test_diagonals PGGL_3_4_lvl_4

# only one orbit, and the diagonal is secant

