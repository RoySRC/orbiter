	$(PROJECTIVE_SPACE_PATH)/analyze.out -v 2 -n 2 -q 16 -poly 19 -file LunelliSce_bsf3.txt



H16_quadrangles:
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2  -PG 2 16 -save PG_2_16.inc
	$(PROJECTIVE_SPACE_PATH)/make_something.out -q 16 -hyperoval 
	$(INC_CAN_PATH)/inc_can.out -v 3 -points_file hyperoval_regular_q16.txt \
		-save_stabilizer \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.035mm" \
		-save_stabilizer \
		PG_2_16.inc
	mv PG_2_16_0_stabilizer.txt hyperoval_regular_q16_stabilizer.txt
	$(INC_CAN_PATH)/read_group.out -v 2 -nb_rows 273 -nb_cols 273 -PG 2 16 \
		-file hyperoval_regular_q16_stabilizer.txt \
		-save 
	$(OYA_PATH)/arc.out -v 2 -q 16   \
		-stab hyperoval_regular_q16_stabilizer_linear.txt \
		-set hyperoval_regular_q16.txt \
		-W -depth 4
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -n 2 -q 16 \
		-file hyperoval_regular_q16.txt \
		-test_diagonals PGGL_3_16_lvl_4
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -n 2 -q 16 -print -points_in_PG \
		-file hyperoval_regular_q16.txt



