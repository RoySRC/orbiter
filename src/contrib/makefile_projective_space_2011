o4:
	$(PROJECTIVE_SPACE_PATH)/ovoid.out -v 2 -q 4
	$(PROJECTIVE_SPACE_PATH)/analyze.out -v 2 -n 3 -q 4 -planes -file ovoid_4.txt -group
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -PG 3 4 -save PG_3_4.inc
	$(INC_CAN_PATH)/inc_can.out -v 6 -points_file ovoid_4.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.010mm" \
		PG_3_4.inc
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -q 4 -n 3 \
		-group -file ovoid_4.txt
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -q 4 -n 3 \
		-line_type -file ovoid_4.txt
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -q 4 -n 3 \
		-plane_type -file ovoid_4.txt


# group order 16320
# line type: ( 0^136, 1^85, 2^136 )
# plane type: ( 1^17, 5^68 )

o4_cone:
	$(PROJECTIVE_SPACE_PATH)/ovoid.out -v 2 -q 4
	$(PROJECTIVE_SPACE_PATH)/process.out -v 2 -q 4 -n 3 \
		-cone -file ovoid_4.txt
	$(INCIDENCE_STRUCTURE_PATH)/tdo.out -v 2 -PG 4 4 -save PG_4_4.inc
	$(INC_CAN_PATH)/inc_can.out -v 6  \
		-points_file ovoid_4_cone.txt \
		-tdo -tdo_steps -tdo_files -tdo_with_scheme \
		-tda -tda_files -tda_with_scheme -tda_with_group_order \
		-unit_length "0.010mm" \
		PG_4_4.inc


