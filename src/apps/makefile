all: 
	cd orbiter; $(MAKE) all
	cd sandbox; $(MAKE) all
	cd geometry_builder; $(MAKE) all
	
no:
	#cd ginac; $(MAKE) all
		# ginac requires cln and ginac to be installed

clean: 
	cd ginac; $(MAKE) clean
	cd orbiter; $(MAKE) clean
	cd sandbox; $(MAKE) clean
	cd geometry_builder; $(MAKE) clean

install: 
	cd orbiter; $(MAKE) install
	cd sandbox; $(MAKE) install

